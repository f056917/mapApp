.start {
  width: 100%;
  min-height: 100vh;
  background-color: #E9DFC8;
  position: relative;
}
.start-close {
  width: 136rpx;
  height: 117rpx;
  position: absolute;
  top: 0;
  right: 0;
}
.start-close image {
  width: 100%;
  height: 100%;
}
.start-main {
  width: 100%;
  padding-bottom: 162rpx;
  box-sizing: border-box;
  position: absolute;
  right: 0;
  bottom: 0;
  overflow: hidden;
  z-index: 100;
}
.start-box {
  width: 326rpx;
  height: 124rpx;
  margin: 0 auto 76rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  overflow: hidden;
}
.start-box-bg {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  right: 0;
  z-index: -1;
}
.start-box-name {
  color: #fff;
  font-size: 40rpx;
  margin: 10rpx 10rpx 0 0;
}