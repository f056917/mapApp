.welcome {
  width: 100%;
  min-height: 100vh;
  padding: 50rpx 60rpx;
  box-sizing: border-box;
  background-color: #E9DFC8;
  position: relative;
  overflow: hidden;
}
.welcome-title {
  margin-bottom: 60rpx;
  display: flex;
  align-items: center;
}
.welcome-title image {
  height: 52rpx;
}
.welcome-title image:nth-child(1) {
  margin-right: 12rpx;
}
.welcome-hint {
  height: 96rpx;
}
.welcome-main {
  width: 666rpx;
  height: 830rpx;
  position: absolute;
  bottom: 0;
  left: 0;
}
.welcome-rabbit {
  width: 100%;
  height: 100%;
}
.welcome-bt {
  height: 94rpx;
  position: absolute;
  top: 270rpx;
  right: 42rpx;
  z-index: 10;
}